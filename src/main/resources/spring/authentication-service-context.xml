<?xml version="1.0" encoding="UTF-8"?>
<beans 	xmlns="http://www.springframework.org/schema/beans"
           xmlns:jaxws="http://cxf.apache.org/jaxws"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="
			http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
			http://cxf.apache.org/jaxws http://cxf.apache.org/schemas/jaxws.xsd">

    <bean id="authInterceptor" class="org.apache.cxf.ws.security.wss4j.WSS4JOutInterceptor">
        <constructor-arg>
            <map>
                <entry key="action" value="UsernameToken Timestamp" />
                <entry key="user" value="${webservice.username}" />
                <entry key="passwordType" value="PasswordText" />
                <entry key="passwordCallbackRef">
                    <bean class="fi.vm.sade.authentication.authorizationsupport.PropertyBackedCallback">
                        <property name="password" value="${webservice.password}" />
                    </bean>
                </entry>
            </map>
        </constructor-arg>
    </bean>

    <!--
    <jaxws:client id="userManagementService"
                  serviceClass="fi.vm.sade.authentication.service.UserManagementService"
                  address="${usermanagement.webservice.url}" />
-->
    <jaxws:client id="userManagementService"
                  serviceClass="fi.vm.sade.authentication.service.UserManagementService"
                  address="http://luokka.hard.ware.fi:8181/cxf/userManagementService" />

</beans>


